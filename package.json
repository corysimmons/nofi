{
  "name": "nofi",
  "version": "1.1.0",
  "license": "MIT",
  "description": "If you're online, it installs/caches npm packages with --force (so you get fresh packages). If you're offline, it installs cached packages with --cache-min.",
  "main": "./cli.js",
  "bin": {
    "nofi": "./cli.js"
  },
  "author": "Cory Simmons <github.cory@mailhero.io>",
  "repository": "https://github.com/corysimmons/nofi",
  "scripts": {
    "start": "babel -w cli.babel.js -o cli.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "is-online": "^5.1.2",
    "meow": "^3.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.8.1",
    "standard": "^8.4.0"
  },
  "optionalDependencies": {
    "jeet": "^6.1.5",
    "lost": "^7.1.0",
    "postcss-ant": "^0.1.4"
  }
}
